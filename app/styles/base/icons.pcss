    }
  }
  &.i-play-in-circle {
    size: 22px;
    background-image: svg-load('play-in-circle.svg', fill=#c3c8cc);
    &.blue {
      background-image: svg-load('play-in-circle.svg', fill=rgb(38, 136, 235));
    }
    &.white {
      background-image: svg-load('play-in-circle.svg', fill=#ffffff);
    }
  }
  &.i-plus {
    size: 14px;
    background-image: svg-load('plus.svg', fill=#c3c8cc);
    &.white {
