.f-ico {
  position: relative;
  display: inline-block;
  transition: all 0.3s ease;
  &.i-chevron {
    size: 14px 9px;
    background-image: svg-load('chevron.svg', fill=#c3c8cc);
    &.white {
      background-image: svg-load('chevron.svg', fill=#fff);
    }
  }
  &.i-message {
    size: 20px 16px;
    background-image: svg-load('message.svg', fill=#fff);
    &.gray {
      background-image: svg-load('message.svg', fill=rgb(195, 200, 204));
    }
  }
  &.i-info {
    size: 22px;
    background-image: svg-load('info.svg', fill=#fff);
  }
  &.i-statistics {
    size: 22px;
    background-image: svg-load('statistics.svg', fill=#2688eb);
    &.white {
      background-image: svg-load('statistics.svg', fill=#fff);
    }
  }
  &.i-loupe {
    size: 22px;
    background-image: svg-load('loupe.svg', fill=#2688eb);
    &.green {
      background-image: svg-load('loupe.svg', fill=rgb(66, 204, 5));
    }
    &.white {
      background-image: svg-load('loupe.svg', fill=#fff);
    }
  }
  &.i-recommendation {
    size: 18px 22px;
    background-image: svg-load('recommendation.svg', fill=#2688eb);
    &.orange {
      background-image: svg-load('recommendation.svg', fill=rgb(255, 153, 0));
    }
    &.white {
      background-image: svg-load('recommendation.svg', fill=#fff);
    }
    &.red {
      background-image: svg-load('recommendation.svg', fill=#ba1a0e);
    }
  }
  &.i-settings {
    size: 22px;
    background-image: svg-load('settings.svg', fill=#2688eb);
    &.gray {
      background-image: svg-load('settings.svg', fill=rgb(195, 200, 204));
    }
    &.white {
      background-image: svg-load('settings.svg', fill=#fff);
    }
  }
  &.i-pause {
    size: 22px;
    background-image: svg-load('pause.svg', fill=#2688eb);
    &.red {
      background-image: svg-load('pause.svg', fill=rgb(244, 44, 44));
    }
    &.gray {
      background-image: svg-load('pause.svg', fill=#c3c8cc);
    }
    &.white {
      background-image: svg-load('pause.svg', fill=#fff);
