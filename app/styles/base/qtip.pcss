.tip-wrap {
  background-color: rgb(255, 255, 255);
  border: 1px solid rgb(233, 239, 240);
  border-radius: 2px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.05);
  &_box {
    padding: 15px;
    font-size: 13px;
    .tip_name {
      margin-bottom: 5px;
      font-size: 16px;
      font-weight: bold;
    }
    .tip_actions {
      margin-top: 5px;
    }
  }
}
